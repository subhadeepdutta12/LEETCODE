69. Sqrt(x)

class Solution {
public:
    int mySqrt(int x) {
        int l = 0, r = x;
        int res = 0;


        while (l <= r) {
            int m = l + ((r - 1) / 2);
            if (static_cast<long long>(m) * m > x) {
                r = m - 1;
            } else if (static_cast<long long>(m) * m < x) {
                l = m + 1;
                res = m;
            } else {
                return m;
            }
        }
        return res;
    }
};
